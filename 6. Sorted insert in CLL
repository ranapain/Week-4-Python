def sorted_insert(head, data):
    new_node = CNode(data)
    if not head:
        new_node.next = new_node
        return new_node
    if data <= head.data:
        temp = head
        while temp.next != head:
            temp = temp.next
        temp.next = new_node
        new_node.next = head
        return new_node
    curr = head
    while curr.next != head and curr.next.data < data:
        curr = curr.next
    new_node.next = curr.next
    curr.next = new_node
    return head
